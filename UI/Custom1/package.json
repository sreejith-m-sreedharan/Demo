{
  "name": "custom",
  "version": "1.0.0",
  "main": "dist/application.js",
  "description": "A minimal Electron application",
  "scripts": {
    "ng": "ng",
    "electron:package": "electron-packager . myapp4 --overwrite --asar --ignore=^e2e$ --ignore=^src$  --ignore=^.editorconfig$ --ignore=^.gitignore$ --ignore=^angular.json$ --ignore=^browserslist$ --ignore=^karma.conf.js$ --ignore=^package-lock.json$ --ignore=^README.md$ --ignore=^tslint --ignore=^tsconfig",
    "start:electron": "electron .",
    "start": "ng serve --proxy-config proxy.conf.json",
    "watch": "nodemon --watch * --exec electron .",
    "elec-rebuild": "electron-rebuild",
    "build:server": "tsc --lib ES2018,DOM --target ES5 application.ts --outDir dist",
    "build": "ng build --base-href ./ && npm run build:server",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.2",
    "@angular/common": "~10.1.2",
    "@angular/compiler": "~10.1.2",
    "@angular/core": "~10.1.2",
    "@angular/forms": "~10.1.2",
    "@angular/platform-browser": "~10.1.2",
    "@angular/platform-browser-dynamic": "~10.1.2",
    "@angular/router": "~10.1.2",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "ng-zorro-antd": "^10.2.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.2",
    "@angular/cli": "~10.1.2",
    "@angular/compiler-cli": "~10.1.2",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.20.14",
    "codelyzer": "^6.0.0",
    "electron": "^13.1.0",
    "electron-packager": "^15.2.0",
    "electron-rebuild": "^2.3.5",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.2"
  }
}
